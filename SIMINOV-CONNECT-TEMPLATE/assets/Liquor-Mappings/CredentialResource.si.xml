<database-mapping-descriptor>

	<table table_name="CREDENTIAL_RESOURCE" class_name="siminov.connect.template.authorization.CredentialResource">
	
		<column variable_name="name" column_name="NAME"> 
			<property name="type">java.lang.String</property>
			<property name="primary_key">true</property>
			<property name="not_null">true</property>
			<property name="unique">true</property>
		</column>

		<column variable_name="value" column_name="VALUE"> 
			<property name="type">java.lang.String</property>
		</column>
		
		<relationships>
   			<many-to-one refer="credential" refer_to="siminov.connect.template.authorization.Credential" on_update="cascade" on_delete="cascade"></many-to-one>		
   		</relationships>
				
	</table>
	
</database-mapping-descriptor>