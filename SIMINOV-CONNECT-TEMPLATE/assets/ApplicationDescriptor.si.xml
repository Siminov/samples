<siminov>

	<property name="name">SIMINOV CONNECT TEMPLATE</property>	
	<property name="description">Siminov Connect Template Application</property>
	<property name="version">0.9</property>

	<property name="load_initially">false</property>

	<!-- DATABASE-DESCRIPTORS -->
	<database-descriptors>
		<database-descriptor>DatabaseDescriptor.si.xml</database-descriptor>
	</database-descriptors>

	
	<service-descriptors>
	    <service-descriptor>Liquor-Services/Liquors.si.xml</service-descriptor>
	    <service-descriptor>Liquor-Services/LiquorBrands.si.xml</service-descriptor>
	</service-descriptors>
	
	
	<!-- SIMINOV EVENTS -->
	<event-handlers>
	    <event-handler>siminov.connect.template.events.SiminovEventHandler</event-handler>
	    <event-handler>siminov.connect.template.events.DatabaseEventHandler</event-handler>
	</event-handlers>

	
	<syn-descriptors>
	    <sync-descriptor>Liquor-Sync/Liquors.si.xml</sync-descriptor>
	    <sync-descriptor>Liquor-Sync/LiquorBrands.si.xml</sync-descriptor>
	</syn-descriptors>
	
	
	<!-- <library-descriptors>
	 	<library-descriptor>siminov.orm.library.template.resources</library-descriptor>   
	</library-descriptors> -->
			
</siminov>






